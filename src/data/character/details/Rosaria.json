            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/c/ce/Talent_Ravaging_Confession.png",
                "name": "Ravaging Confession",
                "type": "Elemental Skill",
                "desc": "Rosaria swiftly positions herself behind her opponent, then pierces and slashes the enemy with her polearm, dealing Cryo DMG.Rosaria cannot use this ability to move behind larger opponents."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/2/26/Talent_Rites_of_Termination.png",
                "name": "Rites of Termination",
                "type": "Elemental Burst",
                "desc": "Rosaria's unique take on this prayer ritual.\nFirst, she swings her weapon to slash surrounding enemies; then, she summons a frigid Ice Lance that strikes the ground. Both actions deal Cryo DMG.\nWhile active, the Ice Lance periodically releases a blast of cold air, dealing Cryo DMG to surrounding enemies."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/e/e3/Talent_Regina_Probationum.png",
                "name": "Regina Probationum",
                "type": "1st Ascension Passive",
                "desc": "When Rosaria strikes an opponent from behind using Ravaging Confession, Rosaria's CRIT Rate increases by 12% for 5s."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/5/53/Talent_Shadow_Samaritan.png",
                "name": "Shadow Samaritan",
                "type": "4th Ascension Passive",
                "desc": "Casting Rites of Termination increases CRIT Rate of all nearby party members (except Rosaria herself) by 15% of Rosaria's CRIT Rate for 10s.CRIT Rate Bonus gained this way cannot exceed 15%."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/c/c8/Talent_Night_Walk.png",
                "name": "Night Walk",
                "type": "Utility Passive",
                "desc": "At night (18:00 \u2013 6:00), increases the Movement SPD of your own party members by 10%.Does not take effect in Domains, Trounce Domains, or Spiral Abyss. Not stackable with Passive Talents that provide the same effects."
            }
