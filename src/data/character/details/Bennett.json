        "talents": [
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/3/38/Talent_Strike_of_Fortune.png",
                "name": "Strike of Fortune",
                "type": "Normal Attack",
                "desc": "Normal Attack: Performs up to 5 rapid strikes.\nCharged Attack: Consumes a certain amount of Stamina to unleash 2 rapid sword swings.\nPlunging Attack: Plunges from mid-air to strike the ground below, damaging opponents along the path and dealing AoE DMG upon impact."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/6/66/Talent_Passion_Overload.png",
                "name": "Passion Overload",
                "type": "Elemental Skill",
                "desc": "Bennett concentrates the spirit of Adventure and Pyro into his blade. Depending on how long he charges for, different effects occur.\nPress: A single, swift flame strike that deals Pyro DMG.\nHold (Short): Charges up, resulting in different effects when unleashed based on the Charge Level\n\nLevel 1: Strikes twice, dealing Pyro DMG and launching opponents.\nLevel 2: Unleashes 3 consecutive attacks that deal impressive Pyro DMG, but the last attack triggers an explosion that launches both Bennett and the enemy.Bennett takes no damage from being launched, but can take fall damage if he falls down a cliff."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/a/a2/Talent_Fantastic_Voyage.png",
                "name": "Fantastic Voyage",
                "type": "Elemental Burst",
                "desc": "Bennett performs a jumping slam attack that deals Pyro DMG, creating an Inspiration Field for 12 seconds.\nInspiration Field:\n\nIf the health of a character within the AoE is equal to or falls below 70%, their health will regenerate each second. The amount of HP restores scales off Bennett's Max HP.\nIf the health of a character within the AoE is higher than 70%, they gain an ATK Bonus that scales based on Bennett's Base ATK.\nImbues characters within the AoE with Pyro."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/7/77/Talent_Rekindle.png",
                "name": "Rekindle",
                "type": "1st Ascension Passive",
                "desc": "Decreases Passion Overload's CD by 20%."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/1/1a/Talent_Fearnaught.png",
                "name": "Fearnaught",
                "type": "4th Ascension Passive",
                "desc": "When inside Fantastic Voyage's circle, Passion Overload's CD is decreased by 50% and Bennett cannot be launched by this skill's explosion."
            },
            {
                "icon": "https://static.wikia.nocookie.net/gensin-impact/images/2/2a/Talent_It_Should_Be_Safe....png",
                "name": "It Should Be Safe...",
                "type": "Utility Passive",
                "desc": "When dispatched on an expedition in Mondstadt, time consumed is reduced by 25%."
            }
